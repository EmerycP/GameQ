<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.emerycprimeau.gameq">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddGame" android:label="@string/addGame"></activity>
        <activity
            android:name=".Completed"
            android:label="@string/Completed" />
        <activity
            android:name=".toComplete"
            android:label="@string/toComplete" />
        <activity
            android:name=".Inscription"
            android:theme="@style/AppThemeLogIn" />
        <activity
            android:name=".LogIn"
            android:theme="@style/AppThemeLogIn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>